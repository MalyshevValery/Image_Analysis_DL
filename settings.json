{
  "data": {
    "images": "Data/Test_Images",
    "masks": "Data/Masks",
    "reg": "Data/Registration",
    "descriptor": "Data/descriptors.csv"
  },
  "registration": {
    "num_images": 4,
    "n_jobs": 5
  },
  "loader_type": "norm",
  "loader": {
    "train_val_test": [
      0.8,
      0.1,
      0.1
    ],
    "shuffle": true
  },
  "aug_all": [
    {
      "name": "float",
      "max_value": 255
      }
  ],
  "aug_train": [
  ],
  "model": {
    "name": "unet"
  },
  "model_compile": {
    "optimizer": "adam",
    "loss": "binary_crossentropy",
    "metrics": [
      "acc",
      "iou"
    ]
  },
  "training": {
    "batch_size": 4,
    "epochs": 5,
    "callbacks": [
      "early_stop",
      "tensorboard",
      "checkpoint",
      "keep_settings"
    ]
  },
  "predict": true
}