{
  "data": {
    "images": "Data/Images",
    "masks": "Data/Masks",
    "descriptor": "Data/descriptors.csv"
  },
  "registration": {
    "num_images": 4,
    "n_jobs": 5
  },
  "generator": {
    "train_val_test": [
      0.8,
      0.1,
      0.1
    ],
    "shuffle": true
  },
  "model": {
    "name": "unet"
  },
  "model_compile": {
    "optimizer": "adam",
    "loss": "binary_crossentropy",
    "metrics": [
      "acc",
      "iou"
    ]
  },
  "training": {
    "batch_size": 4,
    "epochs": 20,
    "callbacks": [
      "early_stop",
      "tensorboard",
      "checkpoint"
    ]
  }
}