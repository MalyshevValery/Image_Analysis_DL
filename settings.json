{
  "data": {
    "images": "Data/Test_Images",
    "masks": "Data/Masks",
    "reg": "Data/Registration",
    "descriptor": "Data/descriptors.csv",
    "input_shape": [
      256,
      256,
      3
    ]
  },
  "registration": {
    "num_images": 1,
    "n_jobs": 4
  },
  "loader_type": "norm",
  "loader": {
    "train_val_test": [
      0.8,
      0.1,
      0.1
    ],
    "shuffle": true
  },
  "aug_all": [
    {
      "name": "clahe"
    },
    {
      "name": "float",
      "max_value": 255
    },
    {
      "name": "resize",
      "height": 256,
      "width": 256
    }
  ],
  "aug_train": [
    {
      "name": "shift_scale_rotate",
      "border_mode": 5,
      "value": 0.0,
      "p": 1
    }
  ],
  "model": {
    "name": "unet"
  },
  "model_compile": {
    "optimizer": "adam",
    "loss": "binary_crossentropy",
    "metrics": [
      "acc",
      "iou"
    ]
  },
  "training": {
    "batch_size": 4,
    "epochs": 5,
    "callbacks": [
      "early_stop",
      "tensorboard",
      "checkpoint",
      "keep_settings"
    ]
  },
  "predict": true,
  "show_sample": true
}