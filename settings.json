{
  "loader": {
    "images": {
      "type": "directory",
      "dir": "Data/Test_Images",
      "color_transform": "to_gray"
    },
    "masks": {
      "type": "directory",
      "dir": "Data/Masks",
      "color_transform": "to_gray"
    },
    "extensions": {
      "image": [],
      "mask": [
        {
          "type": "type_scale",
          "src_max": 255,
          "target_type": "float32",
          "target_max": 1.0
        }
      ],
      "save_image": [
        {
          "type": "type_scale",
          "src_max": 255,
          "target_type": "float32",
          "target_max": 1.0
        }
      ],
      "save": [
        {
          "type": "type_scale",
          "src_max": 1,
          "target_type": "uint8",
          "target_max": 255
        }
      ]
    }
  },
  "registration": {
    "num_images": 1,
    "n_jobs": 4
  },
  "loader_type": "norm",
  "train_val_test": [
    0.8,
    0.1,
    0.1
  ],
  "input_shape": [
    256,
    256,
    1
  ],
  "augmentation_all": [
    {
      "transform": "ToFloat"
    }
  ],
  "augmentation_train": [
    {
      "transform": "Blur"
    },
    {
      "transform": "GaussNoise"
    },
    {
      "transform": "IAASharpen"
    },
    {
      "transform": "ImageCompression",
      "quality_lower": 50
    },
    {
      "transform": "ShiftScaleRotate",
      "border_mode": 1,
      "rotate_limit": 15
    }
  ],
  "model": {
    "name": "unet"
  },
  "model_compile": {
    "optimizer": "adam",
    "loss": "dice",
    "metrics": [
      "loss",
      "iou",
      "acc",
      "f2",
      "precision",
      "recall"
    ]
  },
  "training": {
    "batch_size": 4,
    "epochs": 1,
    "callbacks": [
      "tensorboard",
      "checkpoint",
      "keep_settings"
    ],
    "use_multiprocessing": true,
    "workers": 8
  },
  "predict": true
}