{
  "data": {
    "images": "Data/Test_Images",
    "masks": "Data/Masks",
    "reg": "Data/Registration",
    "descriptor": "Data/descriptors.csv",
    "input_shape": [
      256,
      256,
      3
    ]
  },
  "registration": {
    "num_images": 1,
    "n_jobs": 4
  },
  "loader_type": "norm",
  "loader": {
    "train_val_test": [
      0.8,
      0.1,
      0.1
    ],
    "shuffle": true
  },
  "aug_all": [
    {
      "name": "float",
      "max_value": 255
    }
  ],
  "aug_train": [
    {
      "name": "blur"
    },
    {
      "name": "gauss_noise"
    },
    {
      "name": "iaa_sharpen"
    },
    {
      "name":"compression",
      "quality_lower": 50
    },
    {
      "name": "shift_scale_rotate",
      "border_mode": 1,
      "rotate_limit": 15
    }
  ],
  "model": {
    "name": "unet"
  },
  "model_compile": {
    "optimizer": "adam",
    "loss": "dice",
    "metrics": [
      "iou",
      "acc",
      "f2",
      "precision",
      "recall"
    ]
  },
  "training": {
    "batch_size": 4,
    "epochs": 2,
    "callbacks": [
      "tensorboard",
      "checkpoint",
      "keep_settings"
    ],
    "use_multiprocessing": true,
    "workers": 8
  },
  "predict": true,
  "show_sample": true
}